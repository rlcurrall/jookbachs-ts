{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    },
    "env": {
        "node": true
    },
    "plugins": [
        "prettier",
        "import"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}